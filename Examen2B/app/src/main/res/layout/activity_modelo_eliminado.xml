<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ModeloEliminado">

    <Button
        android:id="@+id/btn_buscarIDModeloEliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Eliminar"
        app:layout_constraintEnd_toEndOf="@+id/tv_nombreMarcaEliminar"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/tv_nombreMarcaEliminar"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombreMarcaEliminar" />

    <EditText
        android:id="@+id/tv_IDModeloEliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:hint="ID Modelo"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/tv_nombreMarcaEliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:hint="Nombre Marca"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_IDModeloEliminar"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/tv_IDModeloEliminar"
        app:layout_constraintTop_toBottomOf="@+id/tv_IDModeloEliminar" />

</androidx.constraintlayout.widget.ConstraintLayout>